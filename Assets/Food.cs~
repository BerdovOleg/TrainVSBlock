using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using TMPro;


public class Food : MonoBehaviour
{
    public int minValue;
    public int maxValue;
    private int value;

    private void Awake()
    {
        minValue = 1;
        maxValue = 4;//Временно
        value = Random.Range(minValue, maxValue);
    }
    // Start is called before the first frame update
    void Start()
    {
        
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
